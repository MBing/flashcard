{
  "name": "flashcard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/actions/**/*.{js,jsx}",
      "src/reducers/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 90,
        "functions": 80,
        "lines": 90
      }
    }
  }
}
